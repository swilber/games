{
  "name": "Donkey Kong",
  "description": "Climb ladders and avoid barrels to reach the top",
  "difficulty": {
    "barrelSpeed": {
      "base": 0.5,
      "multiplier": 0.1,
      "min": 0.8,
      "max": 1.5,
      "formula": "base + (difficulty * multiplier)"
    },
    "barrelFrequency": {
      "value": 150,
      "static": true,
      "source": "config.gameSettings.barrelFrequency"
    },
    "ladderCount": {
      "base": 2,
      "multiplier": 0.5,
      "min": 3,
      "max": 7,
      "formula": "2 + Math.floor(difficulty / 2)"
    },
    "levelCount": {
      "base": 0,
      "multiplier": 0.5,
      "min": 1,
      "max": 5,
      "formula": "Math.floor(difficulty / 2)"
    }
  }
}
