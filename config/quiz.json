{
  "name": "Quiz Game",
  "description": "Answer questions correctly",
  "difficulty": {
    "timeLimit": {
      "base": 30,
      "multiplier": -5,
      "min": 15,
      "max": 30,
      "condition": "difficulty > 7",
      "formula": "difficulty > 7 ? 30 - (difficulty - 7) * 5 : 0"
    },
    "multipleChoice": {
      "value": true,
      "static": true
    }
  }
}
